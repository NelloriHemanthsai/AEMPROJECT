<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <Header
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Header"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Logo_x0020_Path
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Logo Path"
                                        name="./logopath"/>
                                    <Logo_x0020_Mobile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Logo Mobile Image"
                                        name="./logomobileimage"/>
                                    <Logo_x0020_Link
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="LogoLink"
                                        name="./logolink"/>
                                    <Enable_x0020_Switch
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableswitch"
                                        text="please click on ok"/>
                                    <contain
                                        granite:class="contain"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cssClass
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="CSS Class"
                                                name="./cssClass"/>
                                        </items>
                                    </contain>
                                </items>
                            </Header>
                            <Header_x0020_Links
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Header Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Fields
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./details">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Name
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Name"
                                                    name="./name"/>
                                                <Image
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Image"
                                                    name="./image"/>
                                            </items>
                                        </field>
                                    </Fields>
                                </items>
                            </Header_x0020_Links>
                            <SideBar_x0020_Navigation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="SideBar Navigation"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <multifield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./mani">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Desktop_x0020_Icon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Desktop Icon&#xa;"
                                                    name="./desk"/>
                                                <Mobile_x0020_Icon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Mobile Icon&#xa;"
                                                    name="./mob"/>
                                                <nested
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./navi">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Navigation_x0020_URL
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="NavigationURL"
                                                                name="./navisurl"/>
                                                        </items>
                                                    </field>
                                                </nested>
                                            </items>
                                        </field>
                                    </multifield>
                                </items>
                            </SideBar_x0020_Navigation>
                            <Region
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Region"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Country
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Country"
                                        name="./country">
                                        <items jcr:primaryType="nt:unstructured">
                                            <india
                                                jcr:primaryType="nt:unstructured"
                                                text="india"
                                                value="india"/>
                                            <us
                                                jcr:primaryType="nt:unstructured"
                                                text="us"
                                                value="us"/>
                                        </items>
                                    </Country>
                                </items>
                            </Region>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
